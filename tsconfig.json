    "esModuleInterop": true,                            /* CommonJS modüllerinin içe aktarımını kolaylaştırmak için ek JavaScript üretir. */
    // "preserveSymlinks": true,                         /* Sembolik bağlantıların gerçek yoluna çözülmesini engeller. */
    "forceConsistentCasingInFileNames": true,           /* İçe aktarmalarda büyük/küçük harf uyumluluğunu zorunlu kılar. */

    /* Tip Kontrolü */
    "strict": true,                                     /* Tüm sıkı tip kontrolü seçeneklerini etkinleştirir. */
    // "noImplicitAny": true,                            /* 'any' tipine sahip ifadeler ve bildirimler için hata raporlar. */
    // "strictNullChecks": true,                         /* Tip kontrolünde 'null' ve 'undefined' değerlerini dikkate alır. */
    // "strictFunctionTypes": true,                      /* Fonksiyon atamalarında parametre ve dönüş tiplerinin alt tip uyumluluğunu kontrol eder. */
    // "strictBindCallApply": true,                      /* 'bind', 'call' ve 'apply' metodlarının argümanlarını kontrol eder. */
    // "strictPropertyInitialization": true,             /* Sınıf özelliklerinin kurucuda atanıp atanmadığını kontrol eder. */
    // "strictBuiltinIteratorReturn": true,              /* Yerleşik yineleyicilerin 'TReturn' tipini 'undefined' olarak ayarlar. */
    // "noImplicitThis": true,                           /* 'this' ifadesinin 'any' tipinde olmasını engeller. */
    // "useUnknownInCatchVariables": true,               /* catch bloğundaki değişkenlerin varsayılan tipini 'unknown' yapar. */
    // "alwaysStrict": true,                             /* Her zaman 'use strict' ifadesini ekler. */
    // "noUnusedLocals": true,                           /* Kullanılmayan yerel değişkenler için hata raporlar. */
    // "noUnusedParameters": true,                       /* Kullanılmayan fonksiyon parametreleri için hata raporlar. */
    // "exactOptionalPropertyTypes": true,               /* Opsiyonel özellik tiplerini yazıldığı gibi yorumlar. */
    // "noImplicitReturns": true,                        /* Fonksiyonlarda açıkça dönüş yapılmayan kod yolları için hata raporlar. */
    // "noFallthroughCasesInSwitch": true,               /* Switch ifadelerinde düşen durumlar için hata raporlar. */
    // "noUncheckedIndexedAccess": true,                 /* İndeksle erişilen tiplere 'undefined' ekler. */
    // "noImplicitOverride": true,                       /* Türetilmiş sınıflarda override edilen üyelerin 'override' anahtar kelimesiyle işaretlenmesini zorunlu kılar. */
    // "noPropertyAccessFromIndexSignature": true,       /* İndeks tipiyle bildirilen anahtarlar için erişimi zorunlu kılar. */
    "allowUnusedLabels": false,                         /* Kullanılmayan etiketler için hata raporlamasını devre dışı bırakır. */
    "allowUnreachableCode": false,                      /* Ulaşılamayan kodlar için hata raporlamasını devre dışı bırakır. */

    /* Tamamlayıcılık */
    // "skipDefaultLibCheck": true,                      /* Varsayılan .d.ts dosyalarının tip kontrolünü atlar. */
    "skipLibCheck": true                                /* Tüm .d.ts dosyalarının tip kontrolünü atlar. */
 },
